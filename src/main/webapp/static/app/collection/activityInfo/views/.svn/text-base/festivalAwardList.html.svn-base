<div class=" col-md-12 form-horizontal"  id="festivalAwardSelectId" lar-search-block="">	
   <div class="form-horizontal"> 
        <div class="form-group">
            <label for="actFestivalAwardTitle" class="col-xs-2 control-label">奖项名称:</label>
            <div class="col-xs-3">
               <input  id="actFestivalAwardTitle" name="actFestivalAwardTitle" type="text" class="form-control" ng-model="activityInfo.festivalAwardChoose.query.actFestivalAwardTitle">
            </div>           
        
            <label for="actFestivalAwardYear" class="col-xs-2 control-label">年度:</label>
            <div class="col-xs-3">
               <input  id="actFestivalAwardYear" name="actFestivalAwardYear" type="text" class="form-control" ng-model="activityInfo.festivalAwardChoose.query.actFestivalAwardYear" >
            </div>                
            <div class="col-xs-1" style="text-align:center;font-size:14px;line-height:34px;padding-right:0;padding-left:0;">
