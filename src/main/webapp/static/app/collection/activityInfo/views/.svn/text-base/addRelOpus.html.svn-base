<form id="activityOpusRelFormId">
    <div class="col-xs-10 form-horizontal">
        <div class="form-group">
            <label for="activityTitle" class="col-xs-2 control-label">作品分类:</label>
            <div class="col-xs-4">
               <input  id="actOpusClassif" name="actOpusClassif" type="text" class="form-control" ng-model="activityInfo.tempOpusRel.actOpusClassif" >
            </div>           
        </div>        
    </div>
     <div class="col-xs-10  form-horizontal">
        <div class="form-group">
            <label for="actOpusName" class="col-xs-2 control-label">作品名称:</label>
            <div class="col-xs-4">
                  <input id="actOpusName" name="actOpusName" type="text" class="form-control" ng-model="activityInfo.tempOpusRel.actOpusName" >
                    <span> <i class="fa fa-plus" style="position: absolute; right: 20px; top: 10px; title="查询" ng-click="activityInfo.opusChoose.showDialog()" ></i></span>  
            </div>
            <label for="actOpusTag" class="col-xs-2 control-label"><span class="notNull">*</span>作品标签名:</label>
            <div class="col-xs-4">
                <input id="actOpusTag" name="actOpusTag" type="text" class="form-control" ng-model="activityInfo.tempOpusRel.actOpusTag">
            </div>               
</form>