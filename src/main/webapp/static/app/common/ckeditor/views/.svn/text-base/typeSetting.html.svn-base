<div class="col-xs-12" id="typeSettingId" >
	<div class="form-horizontal">
		
		<div class="form-group">
			<label class="col-xs-2 control-label" for="paraPreId">段　　前:</label>
			<div class="col-xs-4">
				<input id="paraPreId" class="form-control" type="number" name="points" min="1" max="100" />
			</div>
			
			<div class="col-xs-2"></div>
			<div class="col-xs-1">
				<input id="firLineIndentId" type="checkbox" >
			</div>
			<label class="col-xs-3" for="firLineIndentId">首行缩进2字:</label>
			
		</div>
		
		<div class="form-group">
			<label class="col-xs-2 control-label" for="paraAfterId">段　　后:</label>
			<div class="col-xs-4">
				<input id="paraAfterId" class="form-control" type="number" name="points" min="1" max="100" />
			</div>
			
			<div class="col-xs-2"></div>
			<div class="col-xs-1">
				<input id="justifyId" type="checkbox" value="checkbox">
			</div>
			<label class="col-xs-3" for="justifyId">两   端   对   齐:</label>
			
		</div>
		
		<div class="form-group">
			<label class="col-xs-2 control-label" for="rowSpanId">行　　距:</label>
			<div class="col-xs-4">
				<input id="rowSpanId" class="form-control" type="number" name="points" min="1" max="100" />
			</div>
			
			<div class="col-xs-2"></div>
			<div class="col-xs-1">
				<input id="phoCenterId" type="checkbox" value="checkbox" >
			</div>
			<label class="col-xs-3" for="phoCenterId">图   片  居   中:</label>
			
		</div>
		
		<br /><br />
		<div class="form-group" >
			<label class="col-xs-2 control-label">字　　体:</label>
			<select name="selFont" id="selFont" class="col-xs-4">
				<option value="宋体">宋体</option>
				<option value="楷体">楷体</option>
				<option value="黑体">黑体</option>
				<option value="Arial">Arial</option>
				<option value="Times New Roman">Times New Roman</option>
				<option value="Verdana">Verdana</option>
			</select>
			
			<label class="col-xs-2 control-label">字　　号:</label>
			<select name="selFontSize" id="selFontSize" class="col-xs-4">
				<option value="8">8</option>
				<option value="9">9</option>
				<option value="10">10</option>
				<option value="11">11</option>
				<option value="12">12</option>
				<option value="14">14</option>
				<option value="16">16</option>
				<option value="18">18</option>
				<option value="20">20</option>
				<option value="22">22</option>
				<option value="24">24</option>
				<option value="26">26</option>
				<option value="28">28</option>
				<option value="36">36</option>
				<option value="48">48</option>
				<option value="72">72</option>
			</select>
		</div>
		
		<br /><br />
		<div class="form-group" >
			<div class="col-xs-8"></div>
			<div class="col-xs-1">
				<input id="remembarSelId" type="checkbox" value="checkbox" >
			</div>
			<label class="col-xs-3" for="remembarSelId">记住我的选择</label>
		</div>
		
		<br />
	</div>
</div>